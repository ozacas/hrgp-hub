<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee         http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
    <display-name>OneKP</display-name>
    <servlet>
        <servlet-name>AvailableDownloads</servlet-name>
        <servlet-class>au.edu.unimelb.plantcell.onekp.services.AvailableDownloads</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileDownload</servlet-name>
        <servlet-class>au.edu.unimelb.plantcell.onekp.services.FileDownload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TreeDownload</servlet-name>
        <servlet-class>au.edu.unimelb.plantcell.onekp.services.TreeDownload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImageCarousel</servlet-name>
        <servlet-class>au.edu.unimelb.plantcell.onekp.services.ImageCarousel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>auto1KPDataComplete</servlet-name>
        <servlet-class>au.edu.unimelb.plantcell.onekp.services.auto1KPDataComplete</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AvailableDownloads</servlet-name>
        <url-pattern>/services/AvailableDownloads</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileDownload</servlet-name>
        <url-pattern>/services/FileDownload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TreeDownload</servlet-name>
        <url-pattern>/services/TreeDownload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImageCarousel</servlet-name>
        <url-pattern>/services/ImageCarousel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>auto1KPDataComplete</servlet-name>
        <url-pattern>/services/auto1KPDataComplete</url-pattern>
    </servlet-mapping>
</web-app>
